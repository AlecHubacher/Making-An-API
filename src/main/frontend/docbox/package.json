{
  "name": "docbox",
  "version": "1.0.11",
  "description": "an api documentation website",
  "main": "index.js",
  "scripts": {
    "gen-index": "cpy --no-overwrite index.html.sample --rename=index.html .",
    "postinstall": "npm run gen-index",
    "start": "budo src/index.js --serve=bundle.js --live",
    "release": "standard-version",
    "test": "npm run test-unit && eslint src && npm run prerender -- /dev/null && rm -rf lib",
    "test-unit": "mocha --require babel-register test",
    "prerender": "babel src --out-dir lib && node lib/render.js",
    "build": "npm run gen-index && cross-env NODE_ENV=production browserify src/index.js | uglifyjs -c -m > bundle.js && npm run prerender -- index.html"
  },
  "browserify": {
    "transform": [
      "babelify",
      "brfs"
    ]
  },
  "keywords": [
    "api",
    "documentation"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^10.0.0",
    "brfs": "2.0.2",
    "browserify": "^17.0.0",
    "cssnano": "^5.0.8",
    "es6-promise": "^4.2.8",
    "eslint": "^8.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.26.1",
    "github-slugger": "^1.4.0",
    "glob-parent": "^6.0.2",
    "highlight.js": "^11.3.1",
    "isomorphic-fetch": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "minifyify": "^7.3.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark": "^14.0.1",
    "remark-html": "^15.0.0",
    "remark-slug": "^7.0.1",
    "unist-util-select": "^4.0.0",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "budo": "^11.6.4",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "expect": "^27.3.1",
    "mocha": "^9.1.3",
    "standard-version": "^9.3.2",
    "uglify-js": "^3.14.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tmcw/docbox.git"
  }
}
